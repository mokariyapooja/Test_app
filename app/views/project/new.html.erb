<h1>Project#new</h1>
<p>Find me in app/views/project/new.html.erb</p>
<%= form_for :Project do |f| %>
  <p>
    <%= f.label :Name %><br>
    <%= f.text_field :text  %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :text %>
  </p>
  
  <p>
    <%= f.label :technology %><br>
    <%= f.text_area :text %>
  </p>


  <p>
    <%= f.label :start_date %><br>
    <%= f.text_area :datetime %>
  </p>


  <p>
    <%= f.label :submission_date %><br>
    <%= f.text_area :datetime %>
  </p>
  
  <p>
    <%= f.submit %> 
  </p>
<% end %>